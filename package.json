{
  "name": "embedgenerator",
  "version": "2.0.1",
  "description": "create url embed for spotify,deezer,youtube,etc...!",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "tsup --watch",
    "test": "jest",
    "build": "tsup --minify",
    "prettier:format": "prettier --write ./",
    "prettier:check": "prettier --check ./",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "release": "npm run build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jokay03J/embedGenerator.git"
  },
  "keywords": [
    "spotify",
    "deezer",
    "youtube",
    "embed",
    "soundcloud"
  ],
  "author": "jokay03J",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jokay03J/embedGenerator/issues"
  },
  "homepage": "https://github.com/Jokay03J/embedGenerator#readme",
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "axios": "^1.3.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.0.2"
  },
  "tsup": {
    "dts": true,
    "bundle": false,
    "treeshake": true,
    "target": "node16",
    "format": [
      "esm",
      "cjs"
    ],
    "entry": [
      "src/**/*.ts"
    ]
  }
}
